{
    "name": "jutge-api-clients",
    "module": "index.ts",
    "type": "module",
    "devDependencies": {
        "@types/bun": "^1.2.3"
    },
    "peerDependencies": {
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "@sinclair/typebox": "^0.34.27",
        "prettier": "^3.5.2",
        "prettier-plugin-organize-imports": "^4.1.0",
        "radash": "^12.1.0"
    },
    "scripts": {
        "gen": "bun run src/index.ts",
        "lint": "bun eslint src",
        "format": "bun prettier src --write",
        "test-python": "(cd src/tests/python && pytest)",
        "test-typescript": "(cd src/tests/typescript && bun test)",
        "test-cpp": "(cd src/tests/cpp && make && ./tests.exe)",
        "test-all": "bun test-python ; bun test-typescript; bun test-cpp"
    }
}
